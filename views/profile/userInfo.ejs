<%- layout('./layouts/boilerplate') %>

<form>
    <div class="dropdown mb-3">
        <label class="col-sm-2 col-form-label">College</label>
        <select class="btn btn-secondary dropdown-toggle 
        style="width:150px;">
            <option>SB Jain</option>
            <option>Another College</option>
        </select>
    </div>
    
    <div class="dropdown mb-3">
        <label class="col-sm-2 col-form-label">Branch</label>

        <select class="btn btn-secondary dropdown-toggle 
        style="width:150px;">
            <option>Computer Science</option>
            <option>Electronics</option>
            <option>Mechanical</option>
        </select>
    </div>
    
    <div class="form-group row mb-3">
      <label class="col-sm-2 col-form-label">Github profile</label>
      <div class="col-sm-3">
        <input type="url" class="form-control"  placeholder="Github profile">
      </div>
    </div>
    <div class="form-group row mb-3">
      <label class="col-sm-2 col-form-label">Instagram</label>
      <div class="col-sm-3">
        <input type="url" class="form-control" placeholder="Instagram ID">
      </div>
    </div>

    <div class="form-group row">
      <div class="col-sm-10">
        <button type="submit" class="btn btn-primary">Save</button>
      </div>
    </div>
  </form>